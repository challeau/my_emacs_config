#include "../minirt.h"

t_object	set_cam(char **attributes, t_scene scene){
	t_object	new;
	t_vec3		pos;
	t_vec3		ori;

	new.pos.x = ft_atof(attributes[1]);
	new.pos.y = ft_atof(attributes[1] + 4);
	new.pos.z = ft_atof(attributes[1] + 4);
	if (new.pos.x < 0 || new.pos.y < 0 || new.pos.z < 0)
		return ((t_object){.type = e_OBJ_NONE});
	new.data.cam.ori.x = ft_atoi(attributes[2]);
	new.data.cam.ori.y = ft_atoi(attributes[2] + 4);
	new.data.cam.ori.z = ft_atoi(attributes[2] + 4);
	if (rgb.x < 0 || rgb.x > 255 || rgb.y < 0 || rgb.y > 255 || rgb.z < 0 || rgb.z > 255)
		return ((t_object){.type = e_OBJ_NONE});

}


#include <stdio.h>

/* int == integer -> 1, 2345, 12234552; */
/* float == decimal ppimt -> 12.4, 245.5; */
/* char == character -> c, d, r; */
/* char * == string of characters -> "hello", "sfhks ;adlsfgj 'slfsf"; */

int	main(__attribute__((unused))int ac, __attribute__((unused))char **av)
{
	char *str = "hello";
	printf("%s\n", str);
	printf("%c\n", *str);
	
	return (0);
}


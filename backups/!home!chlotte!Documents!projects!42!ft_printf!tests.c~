#include <stdio.h>
#include "ft_printf.h"

int main(void)
{
//	static char *s_hidden = "damn good job. bloody good damn good bloody good damn good job. \0 you are an excellent woman \0\0\0\0 this is not supposed to show";
//	static unsigned int mx_u = 235;
//	static char mx_c = 'G';
//	static char *mx_s = "Hello, World!";
//	static int mx_i = 42;
//	static int i_pos_1 = 878023;

	printf("%c", '\0');
	printf("%5c", '\0');
	printf("%-5c", '\0');
	printf("%c", -0);
	printf("%c", 0);
	printf("%c\n", INT_MAX);
	printf("%c\n", 'c');
	printf("%c\n", '\n');
	printf("%c", 'l');
	printf("%c", 'y');
	printf("%c", ' ');
	printf("%c", 'e');
	printf("%c", 's');
	printf("%c", 't');
	printf("%c", ' ');
	printf("%c", 'f');
	printf("%c", 'a');
	printf("%c", 'n');
	printf("%c", 't');
	printf("%c", 'a');
	printf("%c", 's');
	printf("%c", 't');
	printf("%c", 'i');
	printf("%c", 'q');
	printf("%c", 'u');
	printf("%c", 'e');
	printf("%c\n", '!');
	printf("%c\n", '\r');
	printf("%c\n", '\t');
	printf("%i", 42);
	printf("Kashim a %i histoires à raconter", 1001);
	printf("Il fait au moins %i\n", -8000);
	printf("%i", -0);
	printf("%i", 0);
	printf("%i", INT_MAX);
	printf("%i", INT_MIN);
	printf("%%i 0000042 == |%i|\n", 0000042);
	printf("%%i \t == |%i|\n", '\t');
	printf("%%i Lydie == |%i|\n", 'L'+'y'+'d'+'i'+'e');
	printf("%u", 42);
	printf("Kashim a %u histoires à raconter", 1001);
	printf("Il fait au moins %u\n", -8000);
	printf("%u", -0);
	printf("%u", 0);
	printf("%u", INT_MAX);
	printf("%u", INT_MIN);;
	printf("%%u 0000042 == |%u|\n", 0000042);
	printf("%%u \t == |%u|\n", '\t');
	printf("%%u Lydie == |%u|\n", 'L'+'y'+'d'+'i'+'e');
	printf("%x", 42);
	printf("Kashim a %x histoires à raconter", 1001);
	printf("Il fait au moins %x\n", -8000);
	printf("%x", -0);
	printf("%x", 0);
	printf("%x", INT_MAX);
	printf("%x", INT_MIN);
	printf("%%x 0000042 == |%x|\n", 0000042);
	printf("%%x \t == |%x|\n", '\t');
	printf("%%x Lydie == |%x|\n", 'L'+'y'+'d'+'i'+'e');
	printf("%%-5.3s LYDI == |%-5.3s|\n", "LYDI");
	printf("%%04.5i 42 == |%04.5i|\n", 42);
	printf("%%04.3i 42 == |%04.3i|\n", 42);
	printf("%%04.2i 42 == |%04.2i|\n", 42);
	printf("%%04i 42 == |%04i|\n", 42);
	printf("%%05i 42 == |%05i|\n", 42);
	printf("%%0i 42 == |%0i|\n", 42);
	printf("%%0d 0000042 == |%0d|\n", 0000042);
	printf("%%-i 42 == %-i\n", 42);
	printf("%%-d 42 == %-d\n", INT_MIN);
	printf("%%-i -42 == %-i\n", -42);
	printf("%%-4d 42 == |%-4d|\n", 42);
	printf("%%-5d -42 == |%-5d|\n", -42);
	printf("|%3i|%-3i|\n", 42, 42);
	printf("%%-4i 42 == |%-4i|\n", 42);
	printf("%%-*.3s LYDI == |%-*.3s|\n", 5, "LYDI");
	printf("%%*i 42 == |%*i|\n", 5, 42);
	printf("%%*i 42 == |%*i|\n", 3, 42);
	printf("%%*i 42 == |%*i|\n", 2, 42);
	printf("%*s", 32, "abc");
	printf("%-*s", 32, "abc");
	printf("%*s", 3, "hello");
	printf("%.*s", 3, "hello");
	printf("%.*s", 0, "hello");
	printf("%-*.*s", 7, 3, "yolo");
	printf("%*i", 4, 94827);
	printf("%*i", 14, 94827);
	printf("%.*i", 6, -3);
	printf("%0*i", 7, -54);
	printf("%*s", -32, "abc");
	printf("%-*s", -32, "abc");
	printf("%*s", -3, "hello");
	printf("%.*s", -3, "hello");
	printf("%-*.*s", -7, -3, "yolo");
	printf("%*i", -4, 94827);
	printf("%*i", -14, 94827);
	printf("%.*i", -6, -3);
	printf("%0*i", -7, -54);

	return (0);
}
